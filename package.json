{
  "name": "flows",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:setup": "psql $DATABASE_URL -f database/schema.sql && psql $DATABASE_URL -f database/seeds/sites.sql",
    "db:migrate": "psql $DATABASE_URL -f database/migrate.sql",
    "db:refresh-metadata": "node database/refresh-metadata.js",
    "pipeline:test": "ts-node scripts/test-pipeline.ts",
    "pipeline:start": "node -e \"const { DataPipelineScheduler } = require('./src/services/scheduler'); const s = new DataPipelineScheduler(); s.startDataPipeline(); console.log('Pipeline started. Press Ctrl+C to stop.'); process.on('SIGINT', () => { s.stopAllJobs(); process.exit(0); });\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@types/node": "^24.3.0",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.2",
    "eslint": "^9.34.0",
    "eslint-config-next": "^15.5.2",
    "next": "^15.5.2",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tailwindcss": "^4.1.12",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "jest-environment-node": "^30.1.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2"
  }
}
